// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

void reverseArray(int arr[], int n) {
    int i = 0, j = n - 1;
    while (i < j) {
        swap(arr[i], arr[j]);  // built-in swap function
        i++;
        j--;
    }
}

int main() {
    int arr[] = {3, 2, 4, 1, 4, 6};
    int n = 6;

    reverseArray(arr, n);

    for (int x : arr)
        cout << x << " ";
    cout << endl;

    return 0;
}
